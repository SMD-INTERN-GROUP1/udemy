<form
  class="mt-4"
  action="/admin/accounts/<%= admins._id %>?_method=PUT"
  method="POST"
>
  <div class="form-group">
    <label for="exampleInputEmail1" class="fw-bold mb-2">Username:</label>
    <input
      type="text"
      class="form-control mb-2"
      id="exampleInputEmail1"
      aria-describedby="emailHelp"
      name="username"
      value="<%= admins.username %>"
      readonly
    />
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1" class="fw-bold mb-2">Email:</label>
    <!-- <textarea
              class="form-control"
              id="exampleInputPassword1"
              name="description"
            ></textarea> -->
    <input
      type="email"
      class="form-control mb-2"
      id="exampleInputEmail1"
      aria-describedby="emailHelp"
      name="email"
      value="<%= admins.email %>"
      readonly
    />
  </div>
  <div class="d-flex flex-row-reverse">
    <a href="/admin/accounts" type="button" class="btn btn-secondary mt-4 ms-3">
      Back
    </a>
    <button type="submit" class="btn btn-primary mt-4">Submit</button>
  </div>
</form>
