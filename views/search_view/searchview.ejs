<div class="search pt-5" >
    <div class="container">
    <form >
        <div class="sort">
            <select name="price" id="sort" onchange='getSelect()'>
                <option disabled selected>Sort to</option>
                <option value="1">Price from high to low</option>
                <option value="2">Price from low to high</option>
              </select>
          </div>

    </form>
     <div href="" class="search-product">
        <%courses.forEach(function(data) { %>
            <div class="card card-body my-3">
                <div class="media align-items-center text-center text-lg-left flex-column flex-lg-row">
                    <div class="mr-2 mb-3 mb-lg-0"> <img src="<%= data.image %>" width="300" height="180" alt=""> </div>
                    <div class="media-body" style="text-align: left;">
                        <h3 class="media-title font-weight-semibold"><a href="#" data-abc="true" ><%= data.title %></a> </h3>
                        <ul class="list-inline list-inline-dotted mb-3 mb-lg-2">
                            <li class="list-inline-item"><a href="#" class="text-muted" data-abc="true"><%= data.author %></a></li>
                        </ul>
                        <p class="mb-3"><%= data.description %></p></p>
                        <ul class="list-inline list-inline-dotted mb-0">
                            <li class="list-inline-item">All level</li>
                            <li class="list-inline-item">Add to <a href="#" data-abc="true">wishlist</a></li>
                        </ul>
                    </div>
                    <div class="mt-3 mt-lg-0 ml-lg-3 text-center">
                        <h3 class="mb-0 font-weight-semibold">$<%= data.price %></h3>
                        <div style="color: yellow !important;"> <i class="fa fa-star"></i> <i class="fa fa-star"></i> <i class="fa fa-star"></i> <i class="fa fa-star"></i> <i class="fa fa-star"></i> </div>
                        <div class="text-muted">1985 reviews</div> <button type="button" class="btn btn-warning mt-4 text-white"><i class="icon-cart-add mr-2"></i>Add to cart</button>
                      </div>
                </div>
            </div>
         <% }); %>
      
   </div>
    </div>
 </div>